alias codelines='git ls-files | grep -v -e "\.lock" -e "\.json" | xargs wc -l' 
alias mergedbranches='git branch --merged | grep -v \*'
alias cd..=cd ..
alias shutdown='sudo shutdown -h now'
alias dotfiles='cd $DOTFILES'
alias cwg="cargo watch -q -c -x 'run -q'"
alias lg="lazygit"
alias ld="lazydocker"
